{
  "name": "react-exam",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.15.3",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^5.12.1",
    "json-server": "^0.9.5",
    "prettier": "^1.16.0",
    "react-scripts": "0.9.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "linter": "eslint --cache 'src/**/*.js' 'src/**/*.jsx'",
    "linter:fix": "npm run linter -- --fix",
    "pretty": "prettier --write 'src/**/*.js*' && npm run linter:fix",
    "start": "concurrently \"npm run start:api\" \"npm run start:front\"",
    "start:front": "react-scripts start",
    "start:api": "json-server --watch api/db.json --port 3001",
    "test": "react-scripts test --env=jsdom"
  }
}
