{
  "name": "react-exam",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "json-server": "^0.14.2",
    "prettier": "^1.16.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "linter": "eslint --cache 'src/**/*.js' 'src/**/*.jsx'",
    "linter:fix": "npm run linter -- --fix",
    "pretty": "prettier --write 'src/**/*.js*' && npm run linter:fix",
    "start": "concurrently \"npm run start:api\" \"npm run start:front\"",
    "start:front": "react-scripts start",
    "start:api": "json-server --watch api/db.json --port 3001",
    "test": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
